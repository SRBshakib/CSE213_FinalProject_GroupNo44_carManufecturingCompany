<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="681.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Rifat.ProductionManagerIssueSceneController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Issue Details" />
      <VBox layoutX="14.0" layoutY="42.0" spacing="10">
         <children>
            <HBox spacing="10">
               <children>
                  <Label text="Title:" />
                  <TextField fx:id="titleTextField" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox spacing="10">
               <children>
                  <Label text="Description:" />
                  <TextArea fx:id="descriptionTextArea" prefHeight="200.0" prefWidth="400.0" />
               </children>
            </HBox>
            <HBox spacing="10">
               <children>
                  <Button onAction="#submitButtonOnMouseClicked" text="Submit" />
               </children>
            </HBox>
         </children>
      </VBox>
      <TableView fx:id="issueTableView" layoutX="71.0" layoutY="406.0" prefHeight="200.0" prefWidth="400.0">
        <columns>
          <TableColumn fx:id="titleTableColumn" prefWidth="148.0" text="Title" />
          <TableColumn fx:id="descriptionTableColumn" prefWidth="251.0" text="Description" />
        </columns>
      </TableView>
      <Button layoutX="383.0" layoutY="624.0" onAction="#loadButtonOnMouseClicked" text="Load" />
      <Button layoutX="503.0" layoutY="20.0" mnemonicParsing="false" onAction="#backButtonOnMouseClick" text="Return" />
   </children>
</AnchorPane>
