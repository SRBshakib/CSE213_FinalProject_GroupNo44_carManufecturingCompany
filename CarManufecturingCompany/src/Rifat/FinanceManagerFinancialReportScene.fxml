<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Rifat.FinanceManagerFinancialReportSceneController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Financial Report" />
      <VBox layoutX="14.0" layoutY="42.0" spacing="10">
         <children>
            <HBox spacing="10">
               <Label text="Select Report File:" />
               <Button fx:id="selectFileButton" onAction="#selectFile" text="Select File" />
            </HBox>
            <HBox spacing="10">
               <Button fx:id="submitButton" onAction="#submitReport" text="Submit" />
               <Button fx:id="cancelButton" onAction="#cancelReport" text="Cancel" />
            </HBox>
         </children>
      </VBox>
      <Button layoutX="465.0" layoutY="27.0" mnemonicParsing="false" onAction="#backButtonOnMouseClick" text="Return" />
   </children>
</AnchorPane>
