<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" prefHeight="200.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Rifat.ProductionManagerAssemblyManuelSceneController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Select Assembly Manual PDF" />
      <HBox layoutX="14.0" layoutY="42.0" spacing="10">
         <children>
            <Button fx:id="selectPdfButton" onAction="#selectPdf" text="Select PDF" />
            <Button fx:id="sendPdfButton" onAction="#sendPdf" text="Send PDF" />
         </children>
      </HBox>
      <Button layoutX="300.0" layoutY="20.0" mnemonicParsing="false" onAction="#backButtonOnMouseClick" text="Return" />
   </children>
</AnchorPane>
