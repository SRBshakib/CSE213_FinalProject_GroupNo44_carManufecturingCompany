<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Rifat.CarLunchingEventSceneController">
   <children>
      <VBox layoutX="30.0" layoutY="20.0" spacing="20.0">
         <children>
            <Label text="Car Launching Event" />
            <Label text="Date: April 17, 2024" />
            <Label text="Location: Our Showroom" />
            <Label text="Time: 12:00 AM" />
            <Label text="Join us as we unveil our latest car model!" />
            <Button fx:id="learnMoreButton" mnemonicParsing="false" onAction="#learnMoreButtonClicked" text="Learn More" />
         </children>
      </VBox>
   </children>
</AnchorPane>
