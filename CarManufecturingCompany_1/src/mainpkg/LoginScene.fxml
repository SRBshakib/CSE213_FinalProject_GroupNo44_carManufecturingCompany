      <PasswordField fx:id="userPasswordPwfield" layoutX="237.0" layoutY="158.0" prefHeight="25.0" prefWidth="179.0" />
      <Label layoutX="149.0" layoutY="202.0" text="User Type: " />
      <ComboBox fx:id="userTypeComboBox" layoutX="237.0" layoutY="198.0" prefHeight="25.0" prefWidth="179.0" promptText="Select Your Designation" />
      <Hyperlink layoutX="264.0" layoutY="348.0" onAction="#forgotPasswordOnMouseClicked" text="Forgot Password" />
      <Button layoutX="285.0" layoutY="246.0" mnemonicParsing="false" onAction="#signInOnMouseClicked" text="Sign In" />
      <Button layoutX="248.0" layoutY="315.0" mnemonicParsing="false" onAction="#registerAsCustomerOnMouseClicked" text="Register As Customer" />
